{
  "name": "next-starter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "yarn svg-sprite && next dev",
    "build": "yarn svg-sprite && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest -u",
    "test:watch": "jest --watch",
    "check-types": "tsc --pretty --noEmit",
    "prepare": "git config --local core.hooksPath .hooks",
    "storybook": "start-storybook -s ./public -p 9001",
    "build-storybook": "build-storybook",
    "svg-sprite": "svg-sprite --shape-id-separator='' --symbol --symbol-sprite='icons.svg' --symbol-dest='' --dest=public ./src/icons/*.svg"
  },
  "dependencies": {
    "next": "12.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.15.8",
    "@storybook/addon-a11y": "6.3.12",
    "@storybook/addon-actions": "6.3.12",
    "@storybook/addon-essentials": "6.3.12",
    "@storybook/addon-links": "6.3.12",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/builder-webpack5": "6.3.12",
    "@storybook/manager-webpack5": "6.3.12",
    "@storybook/react": "6.3.12",
    "@tailwindcss/aspect-ratio": "0.3.0",
    "@tailwindcss/forms": "0.3.4",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.2",
    "@types/react": "17.0.32",
    "@types/react-test-renderer": "17.0.1",
    "autoprefixer": "10.3.7",
    "babel-jest": "27.3.1",
    "babel-loader": "8.2.3",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "8.3.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.3.1",
    "lint-staged": "11.2.4",
    "next-page-tester": "0.29.1",
    "postcss": "8.3.11",
    "postcss-import": "14.0.2",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.4.1",
    "prettier-plugin-organize-imports": "2.3.4",
    "react-test-renderer": "17.0.2",
    "storybook-addon-next-router": "3.0.8",
    "storybook-addon-outline": "1.4.1",
    "storybook-css-modules-preset": "1.1.1",
    "storybook-dark-mode": "1.0.8",
    "svg-sprite": "1.5.3",
    "tailwindcss": "2.2.17",
    "tailwindcss-children": "2.1.0",
    "tsconfig-paths-webpack-plugin": "3.5.1",
    "typescript": "4.4.4",
    "webpack": "5.59.1"
  }
}
